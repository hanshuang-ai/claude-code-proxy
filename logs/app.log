        console.log('è°ƒç”¨parseLogFileå‡½æ•°...');\n                const result = parseLogFile(fileContent);\n\n                console.log('========== è§£æç»“æœ ==========');\n                console.log('è§£æåˆ°çš„è¯·æ±‚æ•°é‡:', result.requests.length);\n                console.log('è§£æåˆ°çš„æ—¶é—´è·¨åº¦æ•°é‡:', result.timeSpans.length);\n\n                if (result.requests.length > 0) {\n                    console.log('ç¬¬ä¸€ä¸ªè¯·æ±‚é¢„è§ˆ:');\n                    const firstRequest = result.requests[0];\n                    console.log('- è¯·æ±‚ID:', firstRequest.id);\n                    console.log('- LOG-ID:', firstRequest.logId);\n                    console.log('- å¤„ç†æ—¶é—´:', firstRequest.processTime);\n                    console.log('- å¼€å§‹æ—¶é—´:', firstRequest.requestStartTime);\n                    console.log('- ç»“æŸæ—¶é—´:', firstRequest.requestEndTime);\n                    console.log('- Request Headeré•¿åº¦:', firstRequest.requestHeader.length);\n                    console.log('- Request Bodyé•¿åº¦:', firstRequest.requestBody.length);\n                    console.log('- Response Headeré•¿åº¦:', firstRequest.responseHeader.length);\n                    console.log('- Response Bodyé•¿åº¦:', firstRequest.responseBody.length);\n                }\n\n                if (result.timeSpans.length > 0) {\n                    console.log('æ—¶é—´è·¨åº¦é¢„è§ˆ:');\n                    console.log('- ç¬¬ä¸€ä¸ªæ—¶é—´è·¨åº¦:', result.timeSpans[0]);\n                    console.log('- æœ€åä¸€ä¸ªæ—¶é—´è·¨åº¦:', result.timeSpans[result.timeSpans.length - 1]);\n                }\n\n                console.log('============================');\n\n                requestsData = result.requests;\n                timeSpansData = result.timeSpans;\n                currentFileName = fileName;\n\n                console.log('å¼€å§‹æ¸²æŸ“ç•Œé¢...');\n                // æ¸…ç©ºå®¹å™¨å¹¶é‡æ–°æ¸²æŸ“\n                $('#requests-container').empty();\n                renderRequests();\n                updateStatistics();\n\n                console.log('ç•Œé¢æ¸²æŸ“å®Œæˆï¼');\n                // æ˜¾ç¤ºæˆåŠŸæç¤º\n                showSuccessMessage(`æˆåŠŸåŠ è½½æ–‡ä»¶: ${fileName}`);\n            } catch (error) {\n                console.error('========== è§£æå¤±è´¥ ==========');\n                console.error('æ–‡ä»¶å:', fileName);\n                console.error('é”™è¯¯ä¿¡æ¯:', error.message);\n                console.error('é”™è¯¯å †æ ˆ:', error.stack);\n                console.error('============================');\n                showErrorMessage(`è§£ææ–‡ä»¶å¤±è´¥: ${error.message}`);\n            }\n        }\n\n        function toggleContent(partId) {\n            const content = document.getElementById(partId);\n            const icon = document.getElementById('icon-' + partId);\n\n            if (content.classList.contains('hidden')) {\n                content.classList.remove('hidden');\n                icon.classList.add('rotated');\n            } else {\n                content.classList.add('hidden');\n                icon.classList.remove('rotated');\n            }\n        }\n\n        function createRequestGroup(request, index) {\n            const partIdPrefix = `request-${request.id}`;\n\n            // è®¡ç®—çœŸå®å¤„ç†æ—¶é—´\n            let actualProcessTime = request.processTime;\n            if (request.requestStartTime && request.requestEndTime) {\n                const timeDiff = calculateTimeDifferenceInSeconds(request.requestStartTime, request.requestEndTime);\n                actualProcessTime = timeDiff.toFixed(2) + 's';\n            }\n\n            return `\n                <div class=\"request-group\">\n                    <div class=\"request-header\">\n                        <div class=\"request-number\">è¯·æ±‚ #${request.id}</div>\n                        <div class=\"request-meta\">\n                            <div class=\"process-time\">å¤„ç†æ—¶é—´: ${actualProcessTime}</div>\n                            <div class=\"log-id\">LOG-ID: ${request.logId}</div>\n                        </div>\n                    </div>\n                    ${request.requestStartTime && request.requestEndTime ? `\n                    <div class=\"request-time-info\" style=\"font-size: 0.8rem; color: #666; margin-bottom: 15px; text-align: center;\">\n                        å¼€å§‹æ—¶é—´: ${request.requestStartTime} | ç»“æŸæ—¶é—´: ${request.requestEndTime}\n                    </div>\n                    ` : ''}\n                    <div class=\"request-parts\">\n                        <div class=\"part-card\">\n                            <div class=\"part-header request-header-part\" onclick=\"toggleContent('${partIdPrefix}-req-header')\">\n                                <span>Request Header</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-req-header\">â–¼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-req-header\">${formatContent(request.requestHeader, 'requestHeader')}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header\" onclick=\"toggleContent('${partIdPrefix}-req-body')\">\n                                <span>Request Body</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-req-body\">â–¼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-req-body\">${formatContent(request.requestBody, 'requestBody')}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header response-header-part\" onclick=\"toggleContent('${partIdPrefix}-res-header')\">\n                                <span>Response Header</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-res-header\">â–¼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-res-header\">${formatContent(request.responseHeader, 'responseHeader')}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header\" onclick=\"toggleContent('${partIdPrefix}-res-body')\">\n                                <span>Response Body</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-res-body\">â–¼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-res-body\">${formatContent(request.responseBody, 'responseBody')}</div>\n                        </div>\n                    </div>\n                </div>\n            `;\n        }\n\n        // æ¸²æŸ“æ‰€æœ‰è¯·æ±‚\n        function renderRequests() {\n            const container = document.getElementById('requests-container');\n            container.innerHTML = '<h2 class=\"section-title\" style=\"color: white; text-align: center; margin-bottom: 30px;\">æ‰€æœ‰' + requestsData.length + 'ä¸ªHTTPè¯·æ±‚ç»„åˆ</h2>';\n\n            requestsData.forEach((request, index) => {\n                const requestHtml = createRequestGroup(request, index);\n                container.insertAdjacentHTML('beforeend', requestHtml);\n            });\n\n            // æ·»åŠ åŠ è½½åŠ¨ç”»\n            const cards = document.querySelectorAll('.request-group');\n            cards.forEach((card, index) => {\n                card.style.opacity = '0';\n                card.style.transform = 'translateY(20px)';\n                setTimeout(() => {\n                    card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';\n                    card.style.opacity = '1';\n                    card.style.transform = 'translateY(0)';\n                }, index * 50);\n            });\n        }\n\n        // æ›´æ–°ç»Ÿè®¡ä¿¡æ¯\n        function updateStatistics() {\n            if (requestsData.length === 0) return;\n\n            // æ›´æ–°æ€»è¯·æ±‚æ¬¡æ•°\n            document.querySelector('.requests-17').textContent = requestsData.length;\n\n            // è®¡ç®—æ‰§è¡Œæ—¶é—´è·¨åº¦\n            if (timeSpansData.length > 0) {\n                const firstTime = timeSpansData[0];\n                const lastTime = timeSpansData[timeSpansData.length - 1];\n\n                // è®¡ç®—æ—¶é—´å·®\n                const timeDiff = calculateTimeDifference(firstTime.start, lastTime.end);\n                document.querySelector('.time-span').textContent = timeDiff;\n\n                // æ›´æ–°æè¿°\n                document.querySelector('.time-span').parentElement.querySelector('.stat-description').textContent =\n                    `ä» ${firstTime.start} åˆ° ${lastTime.end}`;\n            }\n\n            // è®¡ç®—å¤„ç†æ—¶é—´èŒƒå›´ - ä½¿ç”¨æ¯ä¸ªè¯·æ±‚çš„å®é™…æ—¶é—´å·®ï¼ŒåŒ…å«æ‰€æœ‰æœ‰æ•ˆæ—¶é—´\n            console.log('========== è®¡ç®—å¤„ç†æ—¶é—´èŒƒå›´ ==========');\n            const actualTimes = requestsData.map((req, index) => {\n                console.log(`å¤„ç†è¯·æ±‚ #${req.id}:`);\n                console.log('- å¼€å§‹æ—¶é—´:', req.requestStartTime);\n                console.log('- ç»“æŸæ—¶é—´:', req.requestEndTime);\n                console.log('- å¤„ç†æ—¶é—´:', req.processTime);\n\n                let calculatedTime = 0;\n                if (req.requestStartTime && req.requestEndTime) {\n                    calculatedTime = calculateTimeDifferenceInSeconds(req.requestStartTime, req.requestEndTime);\n                    console.log('- è®¡ç®—çš„æ—¶é—´å·®:', calculatedTime, 'ç§’');\n                } else {\n                    // å¦‚æœæ²¡æœ‰å¼€å§‹å’Œç»“æŸæ—¶é—´ï¼Œå°è¯•ä»å¤„ç†æ—¶é—´è·å–\n                    if (req.processTime && req.processTime.endsWith('s')) {\n                        const processTimeNum = parseFloat(req.processTime.replace('s', ''));\n                        if (!isNaN(processTimeNum) && processTimeNum > 0) {\n                            calculatedTime = processTimeNum;\n                            console.log('- ä»å¤„ç†æ—¶é—´è·å–:', calculatedTime, 'ç§’');\n                        }\n                    }\n                }\n\n                console.log('- æœ€ç»ˆæ—¶é—´å·®:', calculatedTime, 'ç§’');\n                return calculatedTime;\n            }).filter(time => time >= 0); // åŒ…å«0å’Œæœ‰æ•ˆæ—¶é—´\n\n            console.log('æ‰€æœ‰æœ‰æ•ˆæ—¶é—´å·®:', actualTimes);\n            console.log('æœ‰æ•ˆæ—¶é—´å·®æ•°é‡:', actualTimes.length);\n            console.log('============================');\n\n            if (actualTimes.length > 0) {\n                const minTime = Math.min(...actualTimes);\n                const maxTime = Math.max(...actualTimes);\n                document.querySelector('.performance').textContent = `${minTime.toFixed(2)}-${maxTime.toFixed(2)}s`;\n\n                // æ›´æ–°å¹³å‡å“åº”æ—¶é—´æè¿°\n                const avgTime = (actualTimes.reduce((a,b) => a+b, 0) / actualTimes.length).toFixed(2);\n                document.querySelector('.performance').parentElement.querySelector('.stat-description').textContent =\n                    `å¹³å‡å“åº”æ—¶é—´çº¦${avgTime}ç§’`;\n            }\n        }\n\n        // è®¡ç®—æ—¶é—´å·®ï¼ˆè¿”å›ç§’æ•°ï¼‰\n        function calculateTimeDifferenceInSeconds(startTime, endTime) {\n            console.log('========== è®¡ç®—æ—¶é—´å·® ==========');\n            console.log('å¼€å§‹æ—¶é—´:', startTime);\n            console.log('ç»“æŸæ—¶é—´:', endTime);\n\n            // è§£ææ—¶é—´æ ¼å¼ï¼Œæ”¯æŒ YYYY-MM-DD HH:MM:SS æ ¼å¼\n            const parseTime = (timeStr) => {\n                // å¦‚æœæ—¶é—´å­—ç¬¦ä¸²åŒ…å«æ—¥æœŸéƒ¨åˆ†\n                if (timeStr.includes('-')) {\n                    // æ ¼å¼: YYYY-MM-DD HH:MM:SS\n                    const dateTimeMatch = timeStr.match(/(\\d{4})-(\\d{2})-(\\d{2})\\s+(\\d{2}):(\\d{2})