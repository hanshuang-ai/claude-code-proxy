: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n            min-height: 100vh;\n            padding: 20px;\n        }\n\n        .container {\n            max-width: 1400px;\n            margin: 0 auto;\n        }\n\n        .header {\n            text-align: center;\n            color: white;\n            margin-bottom: 30px;\n        }\n\n        .header h1 {\n            font-size: 2.5rem;\n            margin-bottom: 10px;\n            text-shadow: 2px 2px 4px rgba(0,0,0,0.3);\n        }\n\n        .header p {\n            font-size: 1.1rem;\n            opacity: 0.9;\n        }\n\n        .stats-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n            gap: 20px;\n            margin-bottom: 30px;\n        }\n\n        .stat-card {\n            background: white;\n            border-radius: 15px;\n            padding: 25px;\n            box-shadow: 0 10px 30px rgba(0,0,0,0.2);\n            text-align: center;\n            transition: transform 0.3s ease, box-shadow 0.3s ease;\n        }\n\n        .stat-card:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 15px 40px rgba(0,0,0,0.3);\n        }\n\n        .stat-number {\n            font-size: 3rem;\n            font-weight: bold;\n            margin-bottom: 10px;\n        }\n\n        .stat-label {\n            font-size: 1.1rem;\n            color: #666;\n            margin-bottom: 5px;\n        }\n\n        .stat-description {\n            font-size: 0.9rem;\n            color: #999;\n        }\n\n        .requests-1 { color: #FF6B6B; }\n        .requests-17 { color: #4ECDC4; }\n        .time-2min { color: #45B7D1; }\n        .performance { color: #96CEB4; }\n\n        .main-content {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n            gap: 20px;\n        }\n\n        .section-card {\n            background: white;\n            border-radius: 15px;\n            padding: 25px;\n            box-shadow: 0 10px 30px rgba(0,0,0,0.2);\n        }\n\n        .section-title {\n            font-size: 1.5rem;\n            margin-bottom: 20px;\n            color: #333;\n            border-bottom: 3px solid #667eea;\n            padding-bottom: 10px;\n        }\n\n        .request-item, .response-item {\n            background: #f8f9fa;\n            border-radius: 10px;\n            padding: 15px;\n            margin-bottom: 15px;\n            border-left: 4px solid #667eea;\n        }\n\n        .request-item {\n            border-left-color: #FF6B6B;\n        }\n\n        .response-item {\n            border-left-color: #4ECDC4;\n        }\n\n        .item-title {\n            font-weight: bold;\n            color: #333;\n            margin-bottom: 10px;\n            font-size: 1.1rem;\n        }\n\n        .item-content {\n            font-family: 'Courier New', monospace;\n            background: #2d3748;\n            color: #e2e8f0;\n            padding: 15px;\n            border-radius: 8px;\n            font-size: 0.9rem;\n            overflow-x: auto;\n            white-space: pre-wrap;\n            max-height: 300px;\n            overflow-y: auto;\n        }\n\n        .toggle-btn {\n            background: #667eea;\n            color: white;\n            border: none;\n            padding: 8px 16px;\n            border-radius: 5px;\n            cursor: pointer;\n            font-size: 0.9rem;\n            margin-bottom: 10px;\n            transition: background 0.3s ease;\n        }\n\n        .toggle-btn:hover {\n            background: #5a67d8;\n        }\n\n        .hidden {\n            display: none;\n        }\n\n        .requests-container {\n            display: grid;\n            gap: 30px;\n        }\n\n        .request-group {\n            background: white;\n            border-radius: 15px;\n            padding: 25px;\n            box-shadow: 0 10px 30px rgba(0,0,0,0.2);\n        }\n\n        .request-header {\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n            margin-bottom: 20px;\n            padding-bottom: 15px;\n            border-bottom: 2px solid #667eea;\n        }\n\n        .request-number {\n            font-size: 1.5rem;\n            font-weight: bold;\n            color: #667eea;\n        }\n\n        .request-meta {\n            display: flex;\n            gap: 20px;\n            align-items: center;\n        }\n\n        .process-time {\n            background: #e8f5e8;\n            color: #2d7a2d;\n            padding: 5px 12px;\n            border-radius: 20px;\n            font-size: 0.9rem;\n            font-weight: bold;\n        }\n\n        .log-id {\n            background: #f0f4ff;\n            color: #4a5568;\n            padding: 5px 12px;\n            border-radius: 20px;\n            font-size: 0.8rem;\n            font-family: monospace;\n        }\n\n        .request-parts {\n            display: flex;\n            flex-direction: column;\n            gap: 20px;\n        }\n\n        .part-card {\n            background: #f8f9fa;\n            border-radius: 10px;\n            overflow: hidden;\n        }\n\n        .part-header {\n            background: #667eea;\n            color: white;\n            padding: 12px 15px;\n            font-weight: bold;\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n            cursor: pointer;\n            transition: background 0.3s ease;\n        }\n\n        .part-header:hover {\n            background: #5a67d8;\n        }\n\n        .part-header.request-header-part {\n            background: #FF6B6B;\n        }\n\n        .part-header.request-header-part:hover {\n            background: #ff5252;\n        }\n\n        .part-header.response-header-part {\n            background: #4ECDC4;\n        }\n\n        .part-header.response-header-part:hover {\n            background: #3db8af;\n        }\n\n        .part-content {\n            padding: 15px;\n            font-family: 'Courier New', monospace;\n            font-size: 0.9rem;\n            background: #2d3748;\n            color: #e2e8f0;\n            overflow-y: auto;\n            white-space: pre-wrap;\n            line-height: 1.5;\n            min-height: 100px;\n        }\n\n        .toggle-icon {\n            transition: transform 0.3s ease;\n        }\n\n        .toggle-icon.rotated {\n            transform: rotate(180deg);\n        }\n\n        .part-content.hidden {\n            display: none;\n        }\n\n        /* 确保内容完全显示，不换行 */\n        .part-content {\n            word-wrap: break-word;\n            overflow-wrap: break-word;\n        }\n\n        .error-message {\n            background: #fff5f5;\n            border: 2px solid #fc8181;\n            color: #c53030;\n            padding: 20px;\n            border-radius: 10px;\n            text-align: center;\n            font-size: 1.1rem;\n            margin: 20px 0;\n        }\n\n\n        @media (max-width: 768px) {\n            .header h1 {\n                font-size: 2rem;\n            }\n\n            .stats-grid {\n                grid-template-columns: repeat(2, 1fr);\n            }\n\n            .main-content {\n                grid-template-columns: 1fr;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <div class=\"header\">\n            <h1>HTTP请求日志可视化分析</h1>\n        </div>\n\n        <div class=\"stats-grid\">\n            <div class=\"stat-card\">\n                <div class=\"stat-number requests-17\">0</div>\n                <div class=\"stat-label\">总请求次数</div>\n                <div class=\"stat-description\">HTTP请求组合总数</div>\n            </div>\n            <div class=\"stat-card\">\n                <div class=\"stat-number time-span\">00:00:00</div>\n                <div class=\"stat-label\">执行时间跨度</div>\n                <div class=\"stat-description\">从首次到最后一次请求</div>\n            </div>\n            <div class=\"stat-card\">\n                <div class=\"stat-number performance\">0.00-0.00s</div>\n                <div class=\"stat-label\">处理时间范围</div>\n                <div class=\"stat-description\">平均响应时间约0.00秒</div>\n            </div>\n        </div>\n\n        <div class=\"requests-container\" id=\"requests-container\">\n            <h2 class=\"section-title\" style=\"color: white; text-align: center; margin-bottom: 30px;\">所有17个HTTP请求组合</h2>\n        </div>\n    </div>\n\n    <script>\n        // 解析日志文件的函数\n        function parseLogFile(logContent) {\n            const lines = logContent.split('\\n');\n            const requests = [];\n\n            let currentRequest = null;\n            let currentSection = null;\n            let sectionContent = [];\n            let globalTimeSpans = []; // 存储所有时间跨度\n\n            for (let i = 0; i < lines.length; i++) {\n                const line = lines[i];\n\n                // 检测请求开始\n                if (line.includes('系统开始代理请求')) {\n                    if (currentRequest) {\n                        // 先保存当前未完成的section内容到上一个请求\n                        if (currentSection && sectionContent.length > 0) {\n                            const cleanContent = sectionContent.filter(l => l.trim()).join('\\n');\n                            currentRequest[currentSection] = cleanContent;\n                            currentSection = null;\n                            sectionContent = [];\n                        }\n                        // 然后保存上一个请求\n                        requests.push(currentRequest);\n                    }\n\n                    // 解析时间信息\n                    let requestStartTime = '';\n                    let requestEndTime = '';\n                    for (let j = i; j < Math.min(i + 10, lines.length); j++) {\n                        if (lines[j].includes('请求开始时间:')) {\n                            requestStartTime = lines[j].split('请求开始时间:')[1].trim();\n                        }\n                        if (lines[j].includes('结果返回时间:')) {\n                            requestEndTime = lines[j].split('结果返回时间:')[1].trim();\n                        }\n                    }\n\n                    // 解析处理时间\n                    let processTime = '';\n                    for (let j = i; j < Math.min(i + 50, lines.length); j++) {\n                        if (lines[j].includes('x-process-time')) {\n                            const timeMatch = lines[j].match(/x-process-time\\s+(\\d+\\.?\\d*)/);\n                            if (timeMatch) {\n                                processTime = parseFloat(timeMatch[1]).toFixed(2) + 's';\n                            }\n                            break;\n                        }\n                    }\n\n                    currentRequest = {\n                        id: requests.length + 1,\n                        logId: '', // 先设为空，后面从Response Header中提取\n                        processTime: processTime,\n                        requestHeader: '',\n                        requestBody: '',\n                        responseHeader: '',\n                        responseBody: '',\n                        requestStartTime: requestStartTime,\n                        requestEndTime: requestEndTime\n                    };\n\n                    // 添加到全局时间跨度\n                    if (requestStartTime && requestEndTime) {\n                        globalTimeSpans.push({ start: requestStartTime, end: requestEndTime });\n                    }\n                }\n\n                // 检测各部分开始\n                if (line.includes('Request Header:')) {\n                    currentSection = 'requestHeader';\n                    sectionContent = [];\n                } else if (line.includes('Request Body:')) {\n                    currentSection = 'requestBody';\n                    sectionContent = [];\n                } else if (line.includes('Response Header:')) {\n                    currentSection = 'responseHeader';\n                    sectionContent = [];\n                } else if (line.includes('Response Body:')) {\n                    currentSection = 'responseBody';\n                    sectionContent = [];\n                } else if (line.includes('--------------------------------------------------------------------------------------')) {\n                    // 分隔线\n                    if (currentSection) {\n                        // 如果是第一次遇到分隔线，且section为空，说明这是section开始的分隔线\n                        if (sectionContent.length === 0) {\n                            // 不做任何事，继续收集内容\n                        } else {\n                            // 这是section结束的分隔线，保存内容\n                            const cleanContent = sectionContent.filter(l => l.trim()).join('\\n');\n                            currentRequest[currentSection] = cleanContent;\n\n                            // 如果是Response Header，尝试从中提取X-LOG-ID\n                            if (currentSection === 'responseHeader' && !currentRequest.logId) {\n                                const logIdMatch = cleanContent.match(/X-LOG-ID\\s+(.+)/);\n                                if (logIdMatch) {\n                                    currentRequest.logId = logIdMatch[1].trim();\n                                }\n                            }\n\n                            currentSection = null;\n                            sectionContent = [];\n                        }\n                    }\n                } else if (currentSection && currentRequest) {\n                    // 收集内容 - 更宽松的条件\n                    if (line.trim() && !line.includes('☆☆☆') && !line.includes('系统开始代理请求')) {\n                        sectionContent.push(line);\n                    }\n                }\n\n\n                            }\n\n            // 添加最后一个请求，并保存最后的section内容\n            if (currentRequest) {\n                // 保存最后一个section的内容\n                if (currentSection && sectionContent.length > 0) {\n                    const cleanContent = sectionContent.filter(l => l.trim()).join('\\n');\n                    currentRequest[currentSection] = cleanContent;\n\n                    // 如果是Response Header，尝试从中提取X-LOG-ID\n                    if (currentSection === 'responseHeader' && !currentRequest.logId) {\n                        const logIdMatch = cleanContent.match(/X-LOG-ID\\s+(.+)/);\n                        if (logIdMatch) {\n                            currentRequest.logId = logIdMatch[1].trim();\n                        }\n                    }\n                }\n\n                // 如果仍然没有找到X-LOG-ID，使用默认值\n                if (!currentRequest.logId) {\n                    currentRequest.logId = `LOG-${requests.length + 1}`;\n                }\n\n                requests.push(currentRequest);\n            }\n\n            // 将时间跨度数据附加到返回结果\n            return { requests, timeSpans: globalTimeSpans };\n        }\n\n        // 加载和解析日志文件\n        let requestsData = [];\n        let timeSpansData = [];\n\n        function loadLogFile() {\n            $.get('logs/TEST任务处理.log', function(data) {\n                const result = parseLogFile(data);\n                requestsData = result.requests;\n                timeSpansData = result.timeSpans;\n                renderRequests();\n                updateStatistics();\n            }).fail(function() {\n                // 如果本地文件加载失败，显示错误信息\n                $('#requests-container').html('<div class=\"error-message\">无法加载日志文件，请确保文件路径正确</div>');\n            });\n        }\n\n        function toggleContent(partId) {\n            const content = document.getElementById(partId);\n            const icon = document.getElementById('icon-' + partId);\n\n            if (content.classList.contains('hidden')) {\n                content.classList.remove('hidden');\n                icon.classList.add('rotated');\n            } else {\n                content.classList.add('hidden');\n                icon.classList.remove('rotated');\n            }\n        }\n\n        function createRequestGroup(request, index) {\n            const partIdPrefix = `request-${request.id}`;\n\n            // 计算真实处理时间\n            let actualProcessTime = request.processTime;\n            if (request.requestStartTime && request.requestEndTime) {\n                const timeDiff = calculateTimeDifferenceInSeconds(request.requestStartTime, request.requestEndTime);\n                actualProcessTime = timeDiff.toFixed(2) + 's';\n            }\n\n            return `\n                <div class=\"request-group\">\n                    <div class=\"request-header\">\n                        <div class=\"request-number\">请求 #${request.id}</div>\n                        <div class=\"request-meta\">\n                            <div class=\"process-time\">处理时间: ${actualProcessTime}</div>\n                            <div class=\"log-id\">LOG-ID: ${request.logId}</div>\n                        </div>\n                    </div>\n                    ${request.requestStartTime && request.requestEndTime ? `\n                    <div class=\"request-time-info\" style=\"font-size: 0.8rem; color: #666; margin-bottom: 15px; text-align: center;\">\n                        开始时间: ${request.requestStartTime} | 结束时间: ${request.requestEndTime}\n                    </div>\n                    ` : ''}\n                    <div class=\"request-parts\">\n                        <div class=\"part-card\">\n                            <div class=\"part-header request-header-part\" onclick=\"toggleContent('${partIdPrefix}-req-header')\">\n                                <span>Request Header</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-req-header\">▼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-req-header\">${request.requestHeader}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header\" onclick=\"toggleContent('${partIdPrefix}-req-body')\">\n                                <span>Request Body</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-req-body\">▼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-req-body\">${request.requestBody}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header response-header-part\" onclick=\"toggleContent('${partIdPrefix}-res-header')\">\n                                <span>Response Header</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-res-header\">▼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-res-header\">${request.responseHeader}</div>\n                        </div>\n                        <div class=\"part-card\">\n                            <div class=\"part-header\" onclick=\"toggleContent('${partIdPrefix}-res-body')\">\n                                <span>Response Body</span>\n                                <span class=\"toggle-icon\" id=\"icon-${partIdPrefix}-res-body\">▼</span>\n                            </div>\n                            <div class=\"part-content hidden\" id=\"${partIdPrefix}-res-body\">${request.responseBody}</div>\n                        </div>\n                    </div>\n                </div>\n            `;\n        }\n\n        // 渲染所有请求\n        function renderRequests() {\n            const container = document.getElementById('requests-container');\n            container.innerHTML = '<h2 class=\"section-title\" style=\"color: white; text-align: center; margin-bottom: 30px;\">所有' + requestsData.length + '个HTTP请求组合</h2>';\n\n            requestsData.forEach((request, index) => {\n                const requestHtml = createRequestGroup(request, index);\n                container.insertAdjacentHTML('beforeend', requestHtml);\n            });\n\n            // 添加加载动画\n            const cards = document.querySelectorAll('.request-group');\n            cards.forEach((card, index) => {\n                card.style.opacity = '0';\n                card.style.transform = 'translateY(20px)';\n                setTimeout(() => {\n                    card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';\n                    card.style.opacity = '1';\n                    card.style.transform = 'translateY(0)';\n                }, index * 50);\n            });\n        }\n\n        // 更新统计信息\n        function updateStatistics() {\n            if (requestsData.length === 0) return;\n\n            // 更新总请求次数\n            document.querySelector('.requests-17').textContent = requestsData.length;\n\n            // 计算执行时间跨度\n            if (timeSpansData.length > 0) {\n                const firstTime = timeSpansData[0];\n                const lastTime = timeSpansData[timeSpansData.length - 1];\n\n                // 计算时间差\n                const timeDiff = calculateTimeDifference(firstTime.start, lastTime.end);\n                document.querySelector('.time-span').textContent = timeDiff;\n\n                // 更新描述\n                document.querySelector('.time-span').parentElement.querySelector('.stat-description').textContent =\n                    `从 ${firstTime.start} 到 ${lastTime.end}`;\n            }\n\n            // 计算处理时间范围 - 使用每个请求的实际时间差\n            const actualTimes = requestsData.map(req => {\n                if (req.requestStartTime && req.requestEndTime) {\n                    return calculateTimeDifferenceInSeconds(req.requestStartTime, req.requestEndTime);\n                }\n                return 0;\n            }).filter(time => time > 0);\n\n            if (actualTimes.length > 0) {\n                const minTime = Math.min(...actualTimes);\n                const maxTime = Math.max(...actualTimes);\n                document.querySelector('.performance').textContent = `${minTime.toFixed(2)}-${maxTime.toFixed(2)}s`;\n\n                // 更新平均响应时间描述\n                const avgTime = (actualTimes.reduce((a,b) => a+b, 0) / actualTimes.length).toFixed(2);\n                document.querySelector('.performance').parentElement.querySelector('.stat-description').textContent =\n                    `平均响应时间约${avgTime}秒`;\n            }\n        }\n\n        // 计算时间差（返回秒数）\n        function calculateTimeDifferenceInSeconds(startTime, endTime) {\n            // 解析时间格式，日志中的时间都是UTC时间格式\n            const parseTime = (timeStr) => {\n                const parts = timeStr.split(':');\n                let hours = parseInt(parts[0]);\n                let minutes = parseInt(parts[1]);\n                let seconds = parseInt(parts[2]);\n\n                return hours * 3600 + minutes * 60 + seconds;\n            };\n\n            const startSeconds = parseTime(startTime);\n            const endSeconds = parseTime(endTime);\n\n            // 计算时间差\n            let diff = endSeconds - startSeconds;\n            if (diff < 0) {\n                // 如果结束时间小于开始时间，说明跨天了\n                diff += 24 * 3600; // 加一天的时间\n            }\n\n            return diff;\n        }\n\n        // 计算时间差（返回格式化字符串）\n        function calculateTimeDifference(startTime, endTime) {\n            const diff = calculateTimeDifferenceInSeconds(startTime, endTime);\n\n            const hours = Math.floor(diff / 3600);\n            const minutes = Math.floor((diff % 3600) / 60);\n            const seconds = diff % 60;\n\n            // 格式化输出\n            if (hours > 0) {\n                return `${hours}小时${minutes}分${seconds}秒`;\n            } else if (minutes > 0) {\n                return `${minutes}分${seconds}秒`;\n            } else {\n                return `${seconds}秒`;\n            }\n        }\n\n        // 页面加载时执行\n        document.addEventListener('DOMContentLoaded', function() {\n            // 添加初始加载动画\n            const cards = document.querySelectorAll('.stat-card, .section-card');\n            cards.forEach((card, index) => {\n                card.style.opacity = '0';\n                card.style.transform = 'translateY(20px)';\n                setTimeout(() => {\n                    card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';\n                    card.style.opacity = '1';\n                    card.style.transform = 'translateY(0)';\n                }, index * 100);\n            });\n\n            // 加载日志文件\n            loadLogFile();\n        });\n\n        function toggleContent(id) {\n            const element = document.getElementById(id);\n            if (element.classList.contains('hidden')) {\n                element.classList.remove('hidden');\n            } else {\n                element.classList.add('hidden');\n            }\n        }\n    </script>\n</body>\n</html>\n","structuredPatch":[{"oldStart":681,"oldLines":6,"newStart":681,"newLines":155,"lines":["             }","         }"," ","+        // 格式化显示内容","+        function formatContent(content, type) {","+            if (!content || content.trim() === '') {","+                return '<span style=\"color: #999;\">(无内容)</span>';","+            }","+","+            // 根据类型进行不同的格式化","+            switch(type) {","+                case 'requestHeader':","+                case 'responseHeader':","+                    return formatHeaders(content);","+                case 'requestBody':","+                case 'responseBody':","+                    return formatBody(content);","+                default:","+                    return escapeHtml(content);","+            }","+        }","+","+        // 格式化HTTP头部","+        function formatHeaders(content) {","+            const lines = content.split('\\n');","+            let formatted = '';","+","+            lines.forEach(line => {","+                if (line.trim()) {","+                    // 查找包含冒号的行（标准的HTTP头部格式）","+                    if (line.includes(':')) {","+                        const colonIndex = line.indexOf(':');","+                        const headerName = line.substring(0, colonIndex);","+                        const headerValue = line.substring(colonIndex + 1);","+","+                        formatted += `<span style=\"color: #4CAF50; font-weight: bold;\">${escapeHtml(headerName.trim())}</span>:<span style=\"color: #2196F3;\">${escapeHtml(headerValue.trim())}</span>\\n`;","+                    } else {","+                        // 处理其他行（如HTTP状态行）","+                        if (line.includes('HTTP/')) {","+                            formatted += `<span style=\"color: #FF9800; font-weight: bold;\">${escapeHtml(line.trim())}</span>\\n`;","+                        } else {","+                            formatted += `<span style=\"color: #ccc;\">${escapeHtml(line.trim())}</span>\\n`;","+                        }","+                    }","+                }","+            });","+","+            return formatted;","+        }","+","+        // 格式化请求/响应体","+        function formatBody(content) {","+            try {","+                // 尝试解析为JSON","+                const jsonData = JSON.parse(content);","+                return formatJson(jsonData);","+            } catch (e) {","+                // 如果不是JSON，检查是否包含JSON片段","+                if (content.includes('{') && content.includes('}')) {","+                    return tryFormatPartialJson(content);","+                }","+                // 如果不是JSON，直接返回转义后的内容","+                return escapeHtml(content);","+            }","+        }","+","+        // 格式化JSON对象","+        function formatJson(obj, indent = 0) {","+            const spaces = '  '.repeat(indent);","+            let result = '';","+","+            if (Array.isArray(obj)) {","+                result += '[\\n';","+                obj.forEach((item, index) => {","+                    result += spaces + '  ';"